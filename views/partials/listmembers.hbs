{{#each members}}

  <section class="ui stacked black segment">
  
    <h3>{{name}}</h3>
    <p>{{assessmentsNumber}} assessment(s)</p>
    <a href="/member/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deletemember/{{id}}/" class="ui icon button">
      <i class="icon trash"></i>
    </a>
  </section>
{{/each}}